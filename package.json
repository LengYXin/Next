{
  "name": "root",
  "version": "0.0.5",
  "private": true,
  "workspaces": [
    "packages/client",
    "packages/live",
    "packages/mobile",
    "packages/www"
  ],
  "scripts": {
    "start": " cross-env DEPLOY_ENV=dev lerna run start",
    "dev:live": "cd packages/live && cross-env DEPLOY_ENV=dev npm start",
    "dev:www": "cd packages/www && cross-env DEPLOY_ENV=dev npm start",
    "dev:mobile": "cd packages/mobile && cross-env DEPLOY_ENV=dev npm start",
    "dev:h5": "cd packages/weapp && npm run dev:h5",
    "dev:weapp": "cd packages/weapp && npm run dev:weapp",
    "build": " cross-env DEPLOY_ENV=dev lerna run build",
    "build:pro": " cross-env DEPLOY_ENV=pro lerna run build",
    "build:www": "cd packages/www && cross-env DEPLOY_ENV=dev npm run generate",
    "build:www_pro": "cd packages/www && cross-env DEPLOY_ENV=pro npm run generate",
    "build:mobile": "cd packages/mobile && cross-env DEPLOY_ENV=dev npm run generate",
    "build:mobile_pro": "cd packages/mobile && cross-env DEPLOY_ENV=pro npm run generate",
    "build:live": "cd packages/live && cross-env DEPLOY_ENV=dev npm run build",
    "build:live_pro": "cd packages/live && cross-env DEPLOY_ENV=pro npm run build",
    "build:weapp": "cd packages/weapp && npm run build:weapp",
    "build:h5": "cd packages/weapp && npm run build:h5",
    "serve": "cd packages/www && npm run serve",
    "serve:mobile": "cd packages/mobile && cross-env DEPLOY_ENV=dev npm run serve",
    "init": "lerna bootstrap",
    "docs": "docsify serve docs",
    "clean": "lerna clean",
    "clean-all": "npm run clean && rimraf node_modules",
    "publish": "lerna publish --skip-npm",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "alife-logger": "^1.8.20",
    "ant-design-vue": "^1.6.5",
    "bowser": "^2.11.0",
    "core-js": "^3.6.5",
    "crypto-js": "^4.0.0",
    "dplayer": "^1.26.0",
    "lodash": "^4.17.20",
    "lodash-decorators": "^6.0.1",
    "mobx": "^5.15.6",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.3.0",
    "mobx-vue": "^2.0.10",
    "moment": "^2.27.0",
    "nprogress": "^0.2.0",
    "qrcode": "^1.4.4",
    "quill": "^1.3.7",
    "rxjs": "^6.6.3",
    "swiper": "~5.4.5",
    "vant": "^2.10.6",
    "viewerjs": "^1.6.2",
    "vue": "^2.6.12",
    "vue-awesome-swiper": "^4.1.1",
    "vue-class-component": "^7.2.5",
    "vue-i18n": "^8.21.1",
    "vue-infinite-loading": "^2.4.5",
    "vue-lazyload": "^1.3.3",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.4.3",
    "vue-template-compiler": "^2.6.12",
    "vue-virtual-scroller": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@types/crypto-js": "^3.1.47",
    "@types/lodash": "^4.14.161",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@vue/test-utils": "^1.1.0",
    "ava": "^3.12.1",
    "babel-jest": "^26.3.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.2",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "jest": "^26.4.2",
    "lerna": "^3.22.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "moment-locales-webpack-plugin": "^1.2.0",
    "postcss-pxtorem": "^5.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "~3.9.7",
    "vue-jest": "^3.0.6",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
